<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.pocket.pocketeducation">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".PocketEducation"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Formulario"
            android:label="@string/title_activity_formulario"
            android:theme="@style/AppTheme"

            />
        <activity
            android:name=".Historico"
            android:label="@string/title_activity_historico"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Notificacao"
            android:label="@string/title_activity_notificacao"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ListaDeTarefa"
            android:label="@string/title_activity_lista_de_tarefa"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>